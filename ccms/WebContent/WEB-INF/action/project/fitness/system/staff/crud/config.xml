<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
		
	</summary>
	
	<log>false</log>

    <transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>	
		<recordset id="query_staff_skill.sql" source="sql" scope="transaction" />
		<recordset id="query-category.sql" source="sql" scope="transaction" />
	</transaction>
    
	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode="table" recordset="query-category.sql" tag="category"/>
			<print mode="table" recordset="query-category.sql" tag="category2"/>
		<print mode="table" recordset="query_staff_skill.sql" tag="skill-list_addfrom"/>
		<print mode="table" recordset="query_staff_skill.sql" tag="skill-list_addfrom2"/>
		<print mode="form" recordset="_request" />
	</output>

</config>

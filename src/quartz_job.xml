        </job>
        <trigger>
            <cron>
                <name>ptDataStatistics</name>
	        	<group>group1</group>
	        	<job-name>job_ptDataStatistics</job-name>
	        	<job-group>group1</job-group>
                <cron-expression>0 0 1 1 * ?</cron-expression>
            </cron>
        </trigger>
        

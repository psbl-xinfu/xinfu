        </job>
        <trigger>
            <cron>
                <name>mcDataStatistics</name>
	        	<group>group1</group>
	        	<job-name>job_mcDataStatistics</job-name>
	        	<job-group>group1</job-group>
                <cron-expression>0 0 1 1 * ?</cron-expression>
            </cron>
        </trigger>
        
        <!-- 私教部个人数据统计 -->

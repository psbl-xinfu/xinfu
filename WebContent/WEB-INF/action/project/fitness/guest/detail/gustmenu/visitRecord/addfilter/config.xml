<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
	</summary>
	
	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-label.sql" source="sql" scope="transaction" />
		<recordset id="query-labelthe.sql" source="sql" scope="transaction" />  
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode="table" recordset="query-label.sql" tag="label-rows"/>
		<print mode="table" recordset="query-labelthe.sql" tag="labelthe-rows"/>
		<print mode="form" recordset="_request"/>
	</output>

</config>

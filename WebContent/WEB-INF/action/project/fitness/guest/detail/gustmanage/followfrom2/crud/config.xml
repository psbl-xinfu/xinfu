<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
	</summary>
	
	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-thecontact.sql" source="sql" scope="transaction" />
		<recordset id="getisgood.sql" source="sql" scope="transaction" />
		<recordset id="query-Hour.sql" source="sql" scope="transaction" />
		<recordset id="query-Moment.sql" source="sql" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.html</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode="table" recordset="query-Hour.sql" tag="hour-list"/>
		<print mode="table" recordset="query-Moment.sql" tag="moment-list"/>
		<print mode="table" recordset="getisgood.sql" tag="isgood-rows"/>
		<print mode="form" recordset="query-thecontact.sql" />
		
	</output>

</config>

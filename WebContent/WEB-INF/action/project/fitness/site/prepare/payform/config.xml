<?xml version='1.0' encoding='UTF-8'?>
<config>

	<summary>
		新增
	</summary>
	
	<log>false</log>

    <request-encoding>UTF-8</request-encoding>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>true</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-OtherPayWay.sql" source="sql" scope="transaction" /> 
		<recordset id="query-sitedef.sql" source="sql" scope="transaction" /> 
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode="table" recordset="query-OtherPayWay.sql" tag="OtherPayWay-list"/>
		<print mode="form" recordset="query-sitedef.sql" />
	</output>
	
</config>
